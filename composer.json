{
    "name": "kalamuna/kalaconfig",
    "type": "drupal-module",
    "description": "Provides a base set of configuration and module dependencies for starting new Drupal projects.",
    "keywords": ["Drupal"],
    "license": "GPL-2.0+",
    "homepage": "https://github.com/kalamuna/kalaconfig",
    "prefer-stable": true,
    "minimum-stability": "dev",
    "support": {
        "issues": "https://github.com/kalamuna/kalaconfig/issues",
        "source": "https://github.com/kalamuna/kalaconfig.git"
    },
    "authors": [{
        "name": "Derek \"Hawkeye Tenderwolf\" DeRaps",
        "homepage": "https://www.drupal.org/user/1571646",
        "role": "Maintainer"
    }],
    "config": {
      "sort-packages": true
    },
    "repositories": {
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    },
    "require": {
        "components/chosen": ">=1.8.2",
        "drupal/admin_toolbar": ">=1.18",
        "drupal/chosen": ">=2.0",
        "drupal/components": ">=1.0",
        "drupal/config_filter": "*",
        "drupal/config_split": ">=1.1",
        "drupal/core": "^8.0",
        "drupal/linkit": "*",
        "drupal/pathauto": ">=1.0",
        "drupal/redirect": ">=1.0",
        "drupal/twig_tweak": ">=2.0",
        "kalamuna/autogrow_textarea": ">=1.0",
        "kalamuna/kalastatic": "*"
    }
}
