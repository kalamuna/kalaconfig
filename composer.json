{
    "name": "kalamuna/kalaconfig",
    "type": "drupal-module",
    "description": "Provides a base set of configuration and module dependencies for starting new Drupal projects. After installing this module once, you may uninstall it but do not remove it as composer dependencies are still needed for the parent project, [kalamuna/drupal-project](https://github.com/kalamuna/drupal-project).",
    "keywords": ["Drupal"],
    "license": "GPL-2.0+",
    "homepage": "https://github.com/kalamuna/kalaconfig",
    "prefer-stable": true,
    "minimum-stability": "dev",
    "support": {
        "issues": "https://github.com/kalamuna/kalaconfig/issues",
        "source": "https://github.com/kalamuna/kalaconfig.git"
    },
    "authors": [{
        "name": "Derek \"Hawkeye Tenderwolf\" DeRaps",
        "homepage": "https://www.drupal.org/user/1571646",
        "role": "Maintainer"
    }],
    "config": {
      "sort-packages": true
    },
    "repositories": {
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    },
    "require": {
        "components/chosen": "*",
        "drupal/admin_toolbar": "*",
        "drupal/chosen": "*",
        "drupal/components": "*",
        "drupal/config_direct_save": "*",
        "drupal/config_filter": "*",
        "drupal/config_split": "*",
        "drupal/core": "^8.0",
        "drupal/linkit": "*",
        "drupal/pathauto": "*",
        "drupal/redirect": "*",
        "drupal/twig_tweak": "*",
        "kalamuna/autogrow_textarea": "*",
        "kalamuna/kalastatic": "*"
    }
}
